{
  "kind": "collectionType",
  "collectionName": "resume_items",
  "info": {
    "singularName": "resume-item",
    "pluralName": "resume-items",
    "displayName": "ResumeItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string"
    },
    "employment": {
      "type": "component",
      "repeatable": true,
      "component": "experience.experience"
    },
    "education": {
      "type": "component",
      "repeatable": true,
      "component": "education.education",
      "required": false,
      "min": 1
    },
    "course": {
      "type": "component",
      "repeatable": true,
      "component": "education.education"
    },
    "contact": {
      "displayName": "Contact",
      "type": "component",
      "repeatable": false,
      "component": "contact.contact"
    },
    "summary": {
      "type": "customField",
      "options": {
        "preset": "toolbarBalloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "skills": {
      "type": "blocks"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "resume_items"
    }
  }
}
